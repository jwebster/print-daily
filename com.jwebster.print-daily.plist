<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.jwebster.print-daily</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/Caskroom/miniconda/base/bin/python3</string>
        <string>/Users/jwebster/code/personal/print-daily/generate_daily.py</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>6</integer>
        <key>Minute</key>
        <integer>30</integer>
    </dict>

    <key>WorkingDirectory</key>
    <string>/Users/jwebster/code/personal/print-daily</string>

    <key>StandardOutPath</key>
    <string>/Users/jwebster/code/personal/print-daily/logs/stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/jwebster/code/personal/print-daily/logs/stderr.log</string>

    <key>RunAtLoad</key>
    <false/>
</dict>
</plist>
